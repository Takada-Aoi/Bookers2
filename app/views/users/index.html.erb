<div>
 <h2>Users</h2>
  <table class ="table">
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <% @users.each do |user| %>
        <td><%=attachment_image_tag user,:profile_image,:fill,40,40,fallback:"no_image.jpg" %></td>
        <td><%= user.name %></td>
        <td><%= link_to "show",user_path(user.id) %></td>
        <% end %>
      </tr>

    </tbody>
  </table>
</div>
